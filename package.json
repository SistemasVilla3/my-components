{
	"name": "my-components",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "netlify dev",
		"dev:web": "vite dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"db:local": "cross-env PRISMA_SCHEMA=./prisma/schema.sqlite.prisma prisma migrate dev",
		"db:local:seed": "cross-env PRISMA_SCHEMA=./prisma/schema.sqlite.prisma prisma db seed",
		"db:pg": "cross-env PRISMA_SCHEMA=./prisma/schema.postgres.prisma prisma migrate deploy",
		"db:pg:seed": "cross-env PRISMA_SCHEMA=./prisma/schema.postgres.prisma prisma db seed"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"@types/node": "^24.10.0",
		"@vitest/browser-playwright": "^4.0.5",
		"playwright": "^1.56.1",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.14",
		"typescript": "^5.9.3",
		"vite": "^7.1.10",
		"vitest": "^4.0.5",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@netlify/functions": "^5.1.0",
		"@prisma/client": "^6.19.0",
		"date-fns": "^4.1.0",
		"fastify": "^5.6.1",
		"fs-extra": "^11.3.2",
		"netlify-cli": "^23.10.0",
		"serverless-http": "^4.0.0",
		"sweetalert": "^2.1.2",
		"sweetalert2": "^11.26.3",
		"tippy": "^0.0.0",
		"tippy.js": "^6.3.7"
	},
	"prisma": {
		"seed": "ts-node --esm prisma/seed.ts"
	}
}
