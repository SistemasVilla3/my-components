{
	"name": "my-components",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "netlify dev",
		"dev:web": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"generate": "PRISMA_SCHEMA=./prisma/schema.prisma prisma generate",
		"db:pg": "PRISMA_SCHEMA=./prisma/schema.prisma prisma migrate deploy",
		"db:pg:seed": "PRISMA_SCHEMA=./prisma/schema.prisma prisma db seed"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"@types/node": "^24.10.0",
		"@vitest/browser-playwright": "^4.0.5",
		"cross-env": "^10.1.0",
		"playwright": "^1.56.1",
		"prisma": "^6.19.0",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.14",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vite": "^7.1.10",
		"vitest": "^4.0.5",
		"vitest-browser-svelte": "^2.0.1"
	},
	"dependencies": {
		"@netlify/functions": "^5.1.0",
		"@prisma/client": "^6.19.0",
		"date-fns": "^4.1.0",
		"fastify": "^5.6.1",
		"fs-extra": "^11.3.2",
		"netlify-cli": "^23.10.0",
		"serverless-http": "^4.0.0",
		"sweetalert": "^2.1.2",
		"sweetalert2": "^11.26.3",
		"tippy": "^0.0.0",
		"tippy.js": "^6.3.7"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
